#include <iostream>

void sumIntersection(const int* arrA, const int* arrB, int size) {
    int sumElem = 0;
    for (int i = 0; i < size; i++) {
        for (int j = 0; j < size; j++) {
            if (arrA[i] == arrB[j]) {
                sumElem += arrB[j];
                break;
            }
        }
    }
    std::cout << "Сумма пересечения массивов А и В: ";
    std::cout << sumElem;
}

int main() {
    setlocale(LC_ALL, "RU");
    int N; 
    std::cout << "Введите размер массивов (N): ";
    std::cin >> N;

    int* A = new int[N]; 
    int* B = new int[N]; 

    std::cout << "Введите " << N << " элементов для массива A:" << std::endl;
    for (int i = 0; i < N; i++) {
        std::cin >> A[i];
    }

    std::cout << "Введите " << N << " элементов для массива B:" << std::endl;
    for (int i = 0; i < N; i++) {
        std::cin >> B[i];
    }
    sumIntersection(A, B, N);

    delete[] A;
    delete[] B;

    return 0;
}

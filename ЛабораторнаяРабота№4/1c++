#include <iostream>

void getIntervalValues(const int* A, int size) {
    int* B = new int[size];
    int countB = 0;

    for (int i = 0; i < size; i++) {
        if (A[i] >= 10 && A[i] <= 50) {
            B[countB] = A[i];
            countB++;
        }
    }

    std::cout << "Элементы массива B: ";
    for (int i = 0; i < countB; i++) {
        std::cout << B[i] << " ";
    }
    std::cout << std::endl;

    delete[] B;
}

int main(){
    int N = 10;
    setlocale(LC_ALL, "RU");

    int* A = new int[N];

    std::cout << "Введите " << N << " элементов для массива A:" << std::endl;
    for (int i = 0; i < N; i++) {
        std::cin >> A[i];
    }

    getIntervalValues(A, N);

    delete[] A;
    return 0;
}

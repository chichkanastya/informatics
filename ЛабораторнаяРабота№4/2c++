#include <iostream>
#include <set>

void IntersectionSum(const std::set<int>& A, const std::set<int>& B) {
    int sumElem = 0;
    for (int num : A) {
        if (B.count(num)) {
            sumElem += num;
        }
    }
    std::cout << "Сумма пересечения множеств: " << sumElem;
}

int main() {
    setlocale(LC_ALL, "RU");
    int N;

    std::cout << "Введите размер множества (N): ";
    std::cin >> N;

    std::set<int> A;
    std::set<int> B;
    std::cout << "Введите "<< N << " элементов множества A:" << std::endl;
    for (int i = 0; i < N; ++i) {
        int element;
        std::cin >> element;
        A.insert(element);
    }

    std::cout << "Введите " << N << " элементов множества B: " << std::endl;
    for (int i = 0; i < N; ++i) {
        int element;
        std::cin >> element;
        B.insert(element);
    }

    IntersectionSum(A, B);


    return 0;
}
